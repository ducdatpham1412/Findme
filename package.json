{
    "name": "findme",
    "version": "0.0.1",
    "private": true,
    "scripts": {
        "postinstall": "cd scripts && sh custom-lib.sh && cd .. && cd ios && pod install && cd .. && npx jetifier",
        "pod-install-m1": "cd ios && arch -x86_64 pod install && cd ..",
        "prepare": "husky install",
        "ios": "react-native run-ios",
        "start": "npx react-native start",
        "test": "jest",
        "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
        "lint:husky": "eslint $(git diff --staged --name-only | grep -E '(.js$|.ts$|.tsx$)')",
        "android": "react-native run-android",
        "android-run-dev-debug": "react-native run-android --variant=developmentDebug --appId 'com.doffy.android.development'",
        "android-run-dev-release": "react-native run-android --variant=developmentRelease --appId 'com.doffy.android.development'",
        "android-run-staging-debug": "react-native run-android --variant=stagingDebug --appId 'com.doffy.android.staging'",
        "android-run-staging-release": "react-native run-android --variant=stagingRelease --appId 'com.doffy.android.staging'",
        "android-run-production-debug": "react-native run-android --variant=productionDebug --appId 'com.doffy.android.production'",
        "android-run-production-release": "react-native run-android --variant=productionRelease --appId 'com.doffy.android.production'",
        "android-build-apk-develop": "rm -rf android/build && export ENVFILE=.env.development && cd android && ./gradlew assembleDevelopmentRelease && cd ..",
        "android-build-apk-staging": "rm -rf android/build && export ENVFILE=.env.staging && cd android && ./gradlew assembleStagingRelease && cd ..",
        "android-build-apk-production": "rm -rf android/build && export ENVFILE=.env.production && cd android && ./gradlew assembleProductionRelease && cd ..",
        "android-build-aab-production": "rm -rf android/build && export ENVFILE=.env.production && cd android && ./gradlew bundleProductionRelease && cd ..",
        "code-push-ios-development": "appcenter codepush release-react -a Doffy-app/Doffy-App -d Development -t 2.1",
        "code-push-ios-staging": "appcenter codepush release-react -a Doffy-app/Doffy-App -d Staging -t 2.1",
        "code-push-ios-production": "appcenter codepush release-react -a Doffy-app/Doffy-App -d Production -t 1.2",
        "code-push-android-development": "appcenter codepush release-react -a Doffy-app/Doffy-Android -d Development -t 1.3",
        "code-push-android-staging": "appcenter codepush release-react -a Doffy-app/Doffy-Android -d Staging -t 2.1",
        "code-push-android-production": "appcenter codepush release-react -a Doffy-app/Doffy-Android -d Production -t 1.2",
        "rebase": "git checkout develop && git pull origin develop && git checkout - && git rebase -",
        "stash": "git stash && yarn rebase && git stash pop",
        "no-edit": "git add . && git commit --amend --no-edit && git push origin head -f",
        "no-verify": "git add . && git commit --amend --no-edit --no-verify && git push origin head -f"
    },
    "dependencies": {
        "@hookform/resolvers": "^2.5.0",
        "@invertase/react-native-apple-authentication": "2.2.1",
        "@react-native-async-storage/async-storage": "^1.14.1",
        "@react-native-community/blur": "^3.6.0",
        "@react-native-community/cameraroll": "^4.1.2",
        "@react-native-community/datetimepicker": "^3.4.3",

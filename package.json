        "@types/react-native-snap-carousel": "^3.8.5",
        "ahooks": "^2.10.8",
        "axios": "^0.21.1",
        "dayjs": "^1.10.7",
        "fbjs": "^3.0.4",
        "husky": "^8.0.1",
        "i18next": "^20.1.0",
